{
    "name": "php-di/kernel",
    "description": "Kernel for PHP-DI applications",
    "keywords": ["php-di", "application", "kernel"],
    "homepage": "http://php-di.org",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "DI\\Kernel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DI\\Kernel\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0",
        "php-di/php-di": "^6.0",
        "mindplay/composer-locator": "^2.1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "minimum-stability": "alpha",
    "prefer-stable": true
}
