{
    "name": "php-di/kernel",
    "description": "Kernel for PHP-DI applications",
    "keywords": ["php-di", "application", "kernel"],
    "homepage": "http://php-di.org",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "DI\\Kernel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DI\\Kernel\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^5.5|^7.0",
        "php-di/php-di": "^5.2",
        "doctrine/cache": "^1.4",
        "puli/repository": "^1.0.0-beta8@beta",
        "puli/discovery": "^1.0.0-beta8@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    }
}
